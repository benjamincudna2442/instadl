{
    "version": 2,
    "framework": null,
    "functions": {
        "api/**/*": {
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/app.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9"
    }
}
